@model FeeDueRequest
@section Head {

    
    <link href="~/css/w3.css" rel="stylesheet" />

}
<style>
    * {
        box-sizing: border-box;
    }

    body {
        font-family: Arial, Helvetica, sans-serif;
    }

    /* Float four columns side by side */
    .column {
        float: left;
        width: 25%;
        padding: 0 10px;
    }

    /* Remove extra left and right margins, due to padding */
    .row {
        margin: 0 -5px;
    }

        /* Clear floats after the columns */
        .row:after {
            content: "";
            display: table;
            clear: both;
        }

    /* Responsive columns */


    /* Style the counter cards */
    .card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
        padding: 16px;
        text-align: center;
        background-color: #f1f1f1;
    }
    .whatappbutton {
        border: 0px;
        background: transparent;
        cursor: pointer;
    }
</style>
<div id="GridDiv">
    <div class="box box-default">

        <div class="box-body">
            <div class="row">
                <div class="col-md-6">
                    <div id="combo" class="form-group">
                        <label>AcadamicYear</label>
                        <select id='cboSelect' class="form-control select2" style="width: 100%;">
                            @foreach (var k in Model.acadamicYearRows)
                            {
                                <option value="@k.Id">@k.Code</option>
                            }
                        </select>

                    </div>
                    <div id="combo" class="form-group">
                        <label>Course</label>
                        <select id='cboSelectcourse' class="form-control select2" style="width: 100%;"></select>

                    </div><!-- /.form-group -->

                </div><!-- /.col -->
                <div class='col-sm-6'>
                    <div class="form-group">
                        <label>Till Term</label>
                        <select id='cboSelectterm' class="form-control select2" style="width: 100%;">
                            <option value="0">---Select---</option>
              
                        </select>
                    </div>
                    <div id="checkall" class="form-group">
                        <label style="width: 100%;">Include All</label>
                        <input type="checkbox" id="all" name="all" value="all">

                    </div>
                </div><!-- /.row -->
            </div><!-- /.box-body -->
            <div class="box-footer">
                <button style="float: right;margin:5px" type="submit" class="btn btn-primary" id="btnPrint">Print</button>
                <button style="float: right;margin:5px" type="submit" class="btn btn-primary" id="btnAddtoList">Retrieve</button>
            </div>
        </div>
    </div>
</div>
<div class="w3-container" id="pardiv">
   
</div>
<script type="text/javascript">
    BitPro.SchoolMate.FeeDue.initializePage();
</script>